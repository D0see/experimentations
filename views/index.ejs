<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Experimenting</title>
    <link rel="stylesheet" href="./css/app.css">
    <link rel="stylesheet" href="./css/piece.css">
    <script src="main.js" defer></script>
</head>
<body>
    <% pieces.map((piece, entityIndex) => { %>
        <div class ="entityContainer">
            <article class="pieceContainer" id="container <%= entityIndex %>">
                    <div class="pieceName">
                        <%= piece.name %>
                    </div>
                    <div class="pieceBody" id="rook" id="container <%= entityIndex %>">
                        <% piece.body.map((row, rowIndex) => { %>
                            <div class="pieceRow" id="container <%= rowIndex %>"><pre><%= row %></pre></div>
                        <% }); %>
                    </div>
            </article>
        </div>
    <% }); %>
    
</body>
</html>